<dependency>
  <groupId>org.glassfish.metro</groupId>
  <artifactId>webservices-rt</artifactId>
  <version>4.0.0</version> <!-- Jakarta EE 10 compatible -->
</dependency>

<dependency>
  <groupId>org.glassfish.metro</groupId>
  <artifactId>webservices-rt</artifactId>
  <version>4.0.0</version> <!-- or latest Jakarta-compatible version -->
</dependency>
<dependency>
  <groupId>org.glassfish.metro</groupId>
  <artifactId>webservices-api</artifactId>
  <version>4.0.0</version>
</dependency>
<plugin>
  <groupId>org.codehaus.mojo</groupId>
  <artifactId>jaxws-maven-plugin</artifactId>
  <version>2.6</version>
  <executions>
    <execution>
      <goals>
        <goal>wsimport</goal>
      </goals>
      <configuration>
        <wsdlUrls>
          <wsdlUrl>http://example.com/service?wsdl</wsdlUrl>
        </wsdlUrls>
        <packageName>com.example.generated</packageName>
      </configuration>
    </execution>
  </executions>
</plugin>
